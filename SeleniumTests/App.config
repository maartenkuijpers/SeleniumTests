<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="UseBrowserArray" value="true" />
    <add key="WebDriversPath" value="WebDrivers" />
    <add key="ScreenshotsFolder" value="Screenshots" />
    <!--
    WebDriverBrowser values are:
    - Firefox
    - Chrome
    - InternetExplorer
    - Safari
    - PhantomJS <default uses PhantomJS as headless browser>
    -->
    <add key="WebDriverBrowserSingle" value="InternetExplorer" />
    <add key="WebDriverBrowserArray" value="Chrome,Firefox,InternetExplorer" />
  </appSettings>
</configuration>
